#include <iostream> 
using namespace std; 
class Student { 
    private: int age; 
    float marks; 
    public: 
    string name; 
    void setDetails(int a, float m) { age = a; 
    marks = m; 
} 
void displayDetails() { 
    cout << "Name:" << name << endl; 
    cout << "Age:" << age << endl; 
    cout << "Marks:" << marks << endl; 
} 
}; 
int main() { 
    Student s1; s1.name = "sravya"; 
    s1.setDetails(18, 92.5); 
    s1.displayDetails(); 
    return 0; 
} 
