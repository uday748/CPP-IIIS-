#include <iostream> 
using namespace std; 
int add(int a, int b = 10) { 
    return a + b; 
} 
int multiply(int a=4, int b = 2) { 
    return a * b; 
} 
int main() { 
    cout << "Multiply with default: " << multiply(3) << endl; 
    cout << "Add with default: " << add(5) << endl; 
    return 0; 
}
